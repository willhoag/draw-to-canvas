{
  "name": "draw-to-canvas",
  "description": "draw from one canvas to another with some sanity checks",
  "version": "0.0.0",
  "author": {
    "name": "Will Hoag",
    "email": "dev@willhoag.com",
    "url": "www.willhoag.com"
  },
  "bugs": {
    "url": "https://github.com/willhoag/draw-to-canvas/issues"
  },
  "dependencies": {
    "force-canvas-context": "0.0.2"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "create-canvas": "^1.0.1",
    "phantomjs": "^1.9.17",
    "standard": "^4.5.3",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.1",
    "tape-run": "^1.0.0"
  },
  "homepage": "git://github.com/willhoag/draw-to-canvas.git",
  "keywords": [
    "canvas",
    "check",
    "html5",
    "draw"
  ],
  "license": "MIT",
  "main": "index.js",
  "browser": "browser.js",
  "maintainers": [
    {
      "name": "devhoag",
      "email": "dev@willhoag.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/willhoag/draw-to-canvas.git"
  },
  "scripts": {
    "build:browser": "browserify -t babelify --standalone forceCanvasContext index.es6 --extension .es6 > browser.js",
    "build:node": "babel index.es6 > index.js",
    "build": "npm run build:node && npm run build:browser",
    "prepublish": "npm test && npm run build",
    "test": "standard index.es6 && browserify -t babelify --extension .es6 test.es6 | tape-run | tap-spec"
  }
}
